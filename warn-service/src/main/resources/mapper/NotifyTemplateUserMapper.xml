<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rhythm.warn.mapper.NotifyTemplateUserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rhythm.warn.entity.NotifyTemplateUserDO">
    <result column="id" property="id" />
    <result column="created_time" property="createdTime" />
    <result column="modified_time" property="modifiedTime" />
    <result column="creator" property="creator" />
    <result column="modifier" property="modifier" />
    <result column="version" property="version" />
    <result column="is_deleted" property="isDeleted" />
        <result column="notify_template_id" property="notifyTemplateId" />
        <result column="notify_user_id" property="notifyUserId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
    id,
    created_time AS createdTime,
    modified_time AS modifiedTime,
    creator,
    modifier,
    version,
    is_deleted AS isDeleted,
        notify_template_id AS notifyTemplateId, notify_user_id AS notifyUserId
    </sql>

</mapper>
